{
  "name": "react-starter-pack",
  "version": "0.1.0",
  "description": "React Starter for faster project initialization",
  "main": "index.js",
  "scripts": {
    "production": "rm -rf dist/*.js* && NODE_ENV=production webpack -p",
    "start": "NODE_ENV=development webpack-dev-server --inline --port 9997",
    "start-server-dev": "NODE_ENV=development nodemon server.js",
    "start-server-prod": "NODE_ENV=production nodemon server.js",
    "prestart-server-staging": "rm -rf dist/*.js* && NODE_ENV=staging webpack -p",
    "start-server-staging": "NODE_ENV=staging nodemon server.js",
    "lint": "eslint app/"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "babel",
    "webpack"
  ],
  "author": "Tanay Pratap",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "cors": "^2.8.1",
    "http-proxy-middleware": "^0.17.4",
    "jsonwebtoken": "^7.3.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express-static-gzip": "^0.2.3",
    "html-webpack-plugin": "^2.22.0",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.18.2",
    "url": "^0.11.0",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
